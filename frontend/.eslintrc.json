{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript",
        "eslint:recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": "off",
        "object-curly-spacing": "off"
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "globals": {
        "JSX": "readonly"
    }
}
